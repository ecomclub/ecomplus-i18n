{
  "name": "@ecomplus/i18n",
  "version": "1.1.0",
  "description": "Tree shakable dictionary for e-commerce JS apps",
  "main": "dist/i18n.min.js",
  "module": "src/index.js",
  "scripts": {
    "generate": "node ./scripts/generate-named-exports",
    "build": "npm run generate && NODE_ENV=production webpack",
    "release": "rm -rf ./dist; npm run build && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/i18n.git"
  },
  "keywords": [
    "ecommerce",
    "dictionary",
    "translations",
    "ecomplus",
    "storefront"
  ],
  "author": "E-Com Club <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/i18n/issues"
  },
  "homepage": "https://github.com/ecomclub/i18n#readme",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-loader": "^8.0.6",
    "husky": "^3.1.0",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
